<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wequan.bu.repository.dao.CategoryMapper">
    <select id="selectByPrimaryKey" resultType="com.wequan.bu.repository.model.Category">
        SELECT *
        FROM ${schema}.wq_course_category cc
        WHERE cc.id = #{id}
    </select>
</mapper>